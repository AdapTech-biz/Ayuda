{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
        {
            "name": "eureka",
            "image": "xyd93/ayuda-eureka:latest",
            "hostname": "eureka",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 6781,
                    "containerPort": 6781
                }
            ],
            "links": [
                "config-server",
                "account-service",
                "zuul-gateway"
            ]
        },
        {
            "name": "config-server",
            "image": "xyd93/ayuda-config:latest",
            "hostname": "config",
            "essential": false,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 8888,
                    "containerPort": 8888
                }
            ],
            "links": [
                "account-service"
            ]
        },
        {
            "name": "zuul-gateway",
            "image": "xyd93/ayuda-zuul:latest",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 6782
                }
            ],
            "links": [
                "account-service",
                "config-server"
            ]
        },
        {
            "name": "account-service",
            "image": "xyd93/ayuda-account:latest",
            "essential": false,
            "memory": 250
        }
    ]
}